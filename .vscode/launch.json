    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/CalculatorCli/bin/Debug/net8.0/CalculatorCli.dll",
            "args": [
                "3 + 4 x 2 % ( 1 - 5 ) ^ 2 ^ 3"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
        
        
    ]
}
